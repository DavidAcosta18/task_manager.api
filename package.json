{
  "name": "digheon.api.base",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=14.16.0"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "nodemon src/http/api.js",
    "deploy": "yarn db-migrate && node src/http/api.js",
    "lint": "eslint ./src/",
    "db-migrate": "sequelize db:migrate",
    "db-seed": "sequelize db:seed:all",
    "db-reset": "sequelize db:migrate:undo:all",
    "db-undo": "sequelize db:migrate:undo",
    "db-refresh": "sequelize db:migrate:undo:all &&  sequelize db:migrate && sequelize db:seed:all",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "cls-hooked": "^4.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "html-pdf": "^3.0.1",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "phantomjs-prebuilt": "^2.1.16",
    "pug": "^3.0.2",
    "randomstring": "^1.2.1",
    "sequelize": "^6.12.5",
    "sequelize-cli": "^6.3.0",
    "validatorjs": "^3.20.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.4.7",
    "jest-extended": "^1.2.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ]
  }
}
